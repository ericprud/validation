     fhir:OperationDefinitionParameterComponent.documentation [ fhir:value "The Identifier(s) of the **target** patient resource; all of these identifiers MUST be present in the located resource\r(The purpose of this property is to ensure that the correct source patient resource is selected)" ] ;
     fhir:OperationDefinitionParameterComponent.type [ fhir:value "Identifier" ]
  ], [
     fhir:OperationDefinitionParameterComponent.name [ fhir:value "result-patient" ] ;
